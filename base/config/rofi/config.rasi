configuration{
    modi: "run,drun,window";
    icon-theme: "Yaru";
    show-icons: true;
    terminal: "gnome-terminal";
    drun-display-format: "{icon} {name}";
    location: 0;
    disable-history: false;
    hide-scrollbar: false;
    display-drun: "Apps ";
    display-run: "Run ";
    display-window: "Window";
    display-Network: "Network";
    sidebar-mode: true;
}

@theme "catppuccin-latte"
